{
  "name": "mocha-tcov-reporter",
  "version": "1.1.2",
  "keywords": [
    "mocha",
    "code coverage",
    "reporter",
    "text",
    "file list",
    "simple",
    "blanket"
  ],
  "description": "code coverage reporter for mocha",
  "main": "lib/index.js",
  "files": [
    "lib/*.js",
    "README.md"
  ],
  "scripts": {
    "test": "istanbul cover _mocha",
    "build": "./node_modules/.bin/babel -d lib -s inline src",
    "watch": "./node_modules/.bin/babel -d lib -s inline -w src",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/holyshared/mocha-tcov-reporter.git"
  },
  "author": "Noritaka Horio <holy.shared.design@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/holyshared/mocha-tcov-reporter/issues"
  },
  "homepage": "https://github.com/holyshared/mocha-tcov-reporter",
  "dependencies": {
    "lodash": "~3.10",
    "mocha": "^2.3.0",
    "sprintf-js": "~1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.3.15",
    "babel-preset-es2015": "^6.3.13",
    "blanket": "~1.2",
    "bluebird": "^3.0.5",
    "chai": "^3.2.0",
    "coffee-script": "~1.10",
    "coveralls": "~2.11",
    "eslint": "^1.4.1",
    "istanbul": "^0.4.1",
    "sinon": "^1.16.1"
  }
}
