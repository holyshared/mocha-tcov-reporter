{
  "name": "mocha-tcov-reporter",
  "version": "1.1.1",
  "keywords": [
    "mocha",
    "code coverage",
    "reporter",
    "text",
    "file list",
    "simple",
    "blanket"
  ],
  "description": "code coverage reporter for mocha",
  "main": "lib/reporter.js",
  "files": [
    "lib/*.js",
    "README.md"
  ],
  "scripts": {
    "test": "multi='spec=- mocha-lcov-reporter=lcov-report.lcov' ./node_modules/mocha/bin/mocha test",
    "build": "./node_modules/.bin/babel -d lib -s inline src",
    "watch": "./node_modules/.bin/babel -d lib -s inline -w src",
    "coveralls": "./node_modules/gulp/bin/gulp.js --require=coffee-script/register coveralls"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/holyshared/mocha-tcov-reporter.git"
  },
  "author": "Noritaka Horio <holy.shared.design@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/holyshared/mocha-tcov-reporter/issues"
  },
  "homepage": "https://github.com/holyshared/mocha-tcov-reporter",
  "dependencies": {
    "lodash": "~3.10",
    "mocha": "^2.3.0",
    "sprintf-js": "~1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.1.2",
    "babel-preset-es2015": "^6.1.2",
    "blanket": "~1.1",
    "bluebird": "^3.0.5",
    "chai": "^3.2.0",
    "coffee-script": "~1.10",
    "coveralls": "~2.11",
    "eslint": "^1.4.1",
    "gulp": "~3.9",
    "gulp-run": "~1.6",
    "mocha-lcov-reporter": "~1.0.0",
    "mocha-multi": "~0.7",
    "sinon": "^1.16.1"
  },
  "config": {
    "blanket": {
      "pattern": "/lib/",
      "data-cover-never": "/node_modules/"
    }
  }
}
